<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hgsoft.carowner.entity.WarningMessage" table="warning_message">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity" />
        </id>
        <property name="obdSn" type="java.lang.String">
            <column name="obdSn"/>
        </property>
        <property name="messageType" type="java.lang.String">
            <column name="messageType"/>
        </property>
        <property name="messageDesc" type="java.lang.String">
            <column name="messageDesc"/>
        </property>
        <property name="messageTime" type="java.util.Date">
            <column name="messageTime"/>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark"/>
        </property>
        <property name="company" type="java.lang.String">
            <column name="company"/>
        </property>
    </class>
</hibernate-mapping>
