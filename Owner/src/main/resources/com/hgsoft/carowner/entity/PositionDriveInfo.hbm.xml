<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hgsoft.carowner.entity.PositionDriveInfo" table="position_drive_info">
        <id name="id" type="java.lang.String">
            <column name="id" not-null="true"/>
            <generator class="assigned" />
        </id>
        <property name="positionInfoId" type="java.lang.String">
            <column name="positionInfoId" not-null="true"/>
        </property>
         <property name="overspeed" type="java.lang.Integer">
            <column name="overspeed"/>
        </property>
        <property name="suddenTurn" type="java.lang.Integer">
            <column name="suddenTurn"/>
        </property>
        <property name="suddenUpSpeed" type="java.lang.Integer">
            <column name="suddenUpSpeed"/>
        </property>
        <property name="suddenDownSpeed" type="java.lang.Integer">
            <column name="suddenDownSpeed"/>
        </property>
        <property name="suddenBrake" type="java.lang.Integer">
            <column name="suddenBrake"/>
        </property>
        <property name="suddenChangeRoad" type="java.lang.Integer">
            <column name="suddenChangeRoad"/>
        </property>
        <property name="speedNotMatch" type="java.lang.Integer">
            <column name="speedNotMatch"/>
        </property>
        <property name="longLowSpeed" type="java.lang.Integer">
            <column name="longLowSpeed"/>
        </property>
        <property name="time" type="java.util.Date">
            <column name="time"/>
        </property>
    </class>
</hibernate-mapping>
